{
  "manifest_version": 2,

  "name": "Youtube App",
  "description": "This extension is meant to store data about the user liked videos for later reference",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Access List"
  },

  "permissions": [
    "storage",
    "*://*.youtube.com/*"
  ],

  
  "background": {
      "scripts": ["background.js"]
  },
  
   "content_scripts": [
    {
      "matches":  ["*://*.youtube.com/*"],
      "css":      ["style_sheet.css"],
      "js":       ["jquery-2.1.4.js",
                  "content_script.js"],
      "run_at":   "document_end"
    }
  ]
}